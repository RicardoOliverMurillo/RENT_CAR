<% include Partials/_Header%>
<div class="container">
    <div class="row">
        <div>
            <form class="form-inline my-2 my-lg-0" action="/resumenInfo" method="GET" >
                <input class="form-control mr-sm-10" name="busqueda" type="search" placeholder="Buscar..." aria-label="Search">
                <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Buscar</button>
            </form>
        </div>
        <!--LIST-->
        <div class="col-md-7">
            <table class="table table-bordered table-hover">
                <thead>
                    <tr>
                        <th>Cantidad</th>
                        <th>Precio Menor</th>
                        <th>Precio Mayor</th>
                        <th>Precio Promedio</th>                            
                    </tr>
                </thead>
                <tbody>
                    <tbody>
                        <tr>
                        <td><%= arrayResult[0]%></td>
                        <td><%= arrayResult[1]%></td>
                        <td><%= arrayResult[2]%></td>
                        <td><%= arrayResult[3]%></td>
                        </tr>
                    </tbody>

                </tbody>
            </table>
        </div>
    </div>
</div>
<% include Partials/_Footer%>